{
	"name": "emptyts",
	"version": "1.0.0",
	"type": "module",
	"description": "",
	"main": "src/index.ts",
	"scripts": {
		"build": "tsc -b -v",
		"clean": "rimraf ./src/**/bin ./bin ./src/**/tsconfig.tsbuildinfo tsconfig.tsbuildinfo",
		"rebuild": "npm run clean && npm run build",
		"buildw": "tsc -b -v -w --preserveWatchOutput -watch --extendedDiagnostics",
		"eslint": "eslint \"./src/**/**.ts\"",
		"dev": "set TS_NODE_FILES=true & node --inspect --loader ts-node/esm src/index.ts",
		"nodemon": "nodemon --watch src/**/*.ts --watch src/**/*.js --exec node --inspect --loader ts-node/esm ./src/index.ts",
		"tsc-watch": "tsc-watch -b -v -i -w --noclear --compiler \"tsc\" --onSuccess \"node --inspect --es-module-specifier-resolution=node ./bin/index.js\""
	},
	"keywords": [],
	"author": "Th√©o MARTEEL",
	"license": "MIT",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^8.3.0",
		"@typescript-eslint/parser": "^8.3.0",
		"eslint": "^9.9.1",
		"typescript-eslint": "^8.3.0",
		"@types/express": "^4.17.21",
		"@types/cors": "^2.8.17",
		"nodemon": "^3.1.4",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"tsc-watch": "^6.2.0",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"express": "^4.19.2",
		"cors": "^2.8.5"
	}
}